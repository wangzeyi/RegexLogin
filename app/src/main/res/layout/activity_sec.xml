<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecActivity"
    tools:layout_editor_absoluteY="73dp">

    <TextView
        android:id="@+id/usr_txt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="19dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintBottom_toTopOf="@+id/email_txt"
        app:layout_constraintEnd_toEndOf="@+id/email_txt"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/email_txt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/dob_txt"
        app:layout_constraintEnd_toEndOf="@+id/dob_txt"
        app:layout_constraintStart_toStartOf="@+id/dob_txt"
        app:layout_constraintTop_toBottomOf="@+id/usr_txt" />

    <TextView
        android:id="@+id/dob_txt"
        android:layout_width="139dp"
        android:layout_height="0dp"
        android:layout_marginBottom="222dp"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_txt" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="142dp"
        android:layout_height="0dp"
        android:layout_marginBottom="17dp"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="29dp"
        android:text="Your Information"
        app:layout_constraintBottom_toTopOf="@+id/usr_txt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>